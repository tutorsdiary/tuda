                    android:layout_height="50dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/end_date_title"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:text="채용마감일"
                        android:gravity="center_vertical"
                        android:textColor="@color/transparent_dark30"
                        style="@style/T3"/>

                    <TextView
                        android:id="@+id/end_date"
                        android:layout_toRightOf="@id/end_date_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingRight="5dp"
                        android:gravity="center_vertical"
                        android:background="@null"
                        android:maxLines="1"
                        android:inputType="none"
                        android:editable="false"
                        android:hint="마감일을 입력하세요."
                        style="@style/T3"/>

                    <include layout="@layout/divider_full"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="75dp"/>
                </RelativeLayout>

                <TextView
                    android:id="@+id/qualification_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="7.5dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:gravity="center_vertical"
                    android:text="응시자격"
                    style="@style/T2"/>

                <include layout="@layout/divider_full"/>

                <RelativeLayout
                    android:id="@+id/career_area"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/career_title"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:text="경력"
                        android:gravity="center_vertical"
                        android:textColor="@color/transparent_dark30"
                        style="@style/T3"/>

                    <TextView
                        android:id="@+id/career"
                        android:layout_toRightOf="@id/career_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingRight="5dp"
                        android:gravity="center_vertical"
                        android:background="@null"
                        android:maxLines="1"
                        android:hint="경력을 선택하세요."
                        android:inputType="none"
                        android:editable="false"
                        style="@style/T3"/>

                    <include layout="@layout/divider_full"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="75dp"/>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/education_area"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/education_title"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:text="학력"
                        android:gravity="center_vertical"
                        android:textColor="@color/transparent_dark30"
                        style="@style/T3"/>

                    <TextView
                        android:id="@+id/education"
                        android:layout_toRightOf="@id/education_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingRight="5dp"
                        android:gravity="center_vertical"
                        android:background="@null"
                        android:maxLines="1"
                        android:hint="학력을 선택하세요."
                        android:inputType="none"
                        android:editable="false"
                        style="@style/T3"/>

                    <include layout="@layout/divider_full"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="75dp"/>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/gender_area"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/gender_title"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:text="성별"
                        android:gravity="center_vertical"
                        android:textColor="@color/transparent_dark30"
                        style="@style/T3"/>

                    <TextView
                        android:id="@+id/gender"
                        android:layout_toRightOf="@id/gender_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingRight="5dp"
                        android:gravity="center_vertical"
                        android:background="@null"
                        android:maxLines="1"
                        android:hint="성별을 선택하세요."
                        android:inputType="none"
                        android:editable="false"
                        style="@style/T3"/>

                    <include layout="@layout/divider_full"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="80dp"/>

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/birthyear_area"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/birthyear_title"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:text="나이제한"
                        android:gravity="center_vertical"
                        android:textColor="@color/transparent_dark30"
                        style="@style/T3"/>

                    <TextView
                        android:id="@+id/birthyear"
                        android:layout_toRightOf="@id/birthyear_title"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingRight="5dp"
                        android:gravity="center_vertical"
                        android:background="@null"
                        android:maxLines="1"
                        android:hint="나이제한을 입력해주세요."
                        android:inputType="none"
                        android:editable="false"
                        style="@style/T3"/>

                    <include layout="@layout/divider_full"
                        android:layout_alignParentBottom="true"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="75dp"/>

                </RelativeLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="7.5dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:gravity="center_vertical"
                    android:text="상세내용"
                    style="@style/T2"/>

                <include layout="@layout/divider_full"/>


                <EditText
                    android:id="@+id/description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:background="@null"
                    android:layout_weight="1"
                    android:hint="과목, 지역, 자격증 유무, 경력사항, 등의 추가정보와 채용과 관련된 상세한 내용을 입력해주세요."
                    style="@style/T3"
                    android:minHeight="120dp"
                    android:inputType="textMultiLine"
                    android:gravity="top"/>

                <include layout="@layout/divider_full"/>

                <LinearLayout
                    android:id="@+id/layout_isAgree"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="12.5dp"
                    android:paddingRight="12.5dp"
                    android:layout_marginBottom="15dp"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingTop="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginRight="10dp"
                            android:text="입력한 정보는 구인•구직 정보 조회시 타인에게 제공될 수 있습니다. 정보제공에 동의하십니까?"
                            style="@style/T3"
                            android:textSize="13dp"/>

                        <CheckBox
                            android:id="@+id/checkbox_isAgree"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical|right"
                            android:checked="true"/>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </ScrollView>
    </LinearLayout>

    <include layout="@layout/progress" />
</FrameLayout>
